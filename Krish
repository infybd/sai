let 
    parts = Text.Split([Date], "-"),
    year = "20" & parts{0},
    month = parts{1},
    day = parts{2}
in
    Date.FromText(year & "-" & month & "-" & day)

Total Exceptions = COUNTROWS(Exceptions)

Records Per App = 
CALCULATE(COUNTROWS(Exceptions), ALLEXCEPT(Exceptions, Exceptions[ApplicationName]))

Missing ExceptionType % = 
DIVIDE(CALCULATE(COUNTROWS(Exceptions), ISBLANK(Exceptions[ExceptionType])), COUNTROWS(Exceptions), 0)
